{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [
            false
        ],
        "no-console": [
            false
        ],
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": [
            false
        ],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ]
    },
    "rulesDirectory": []
}